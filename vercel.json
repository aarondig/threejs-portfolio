{
  "routes": [
    {
      "src": "^/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "^/manifest\\.json$",
      "dest": "/manifest.json"
    },
    {
      "src": "^/favicon\\.ico$",
      "dest": "/favicon.ico"
    },
    {
      "src": "^/logo(.*)\\.png$",
      "dest": "/logo$1.png"
    },
    {
      "src": "^/robots\\.txt$",
      "dest": "/robots.txt"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
